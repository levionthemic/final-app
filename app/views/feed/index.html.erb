<div class="guest-feeds">
  <div class="guest-feeds-wrap">
    <div
      class="btn-group"
      role="group"
      aria-label="Basic radio toggle button group"
    >
      <div>
        <input
          type="radio"
          class="btn-check"
          name="typepage"
          id="photo"
          autocomplete="off"
          checked
        />
        <label class="btn btn-outline-primary" for="photo">PHOTO</label>

      </div>

<div>

      <input
        type="radio"
        class="btn-check"
        name="typepage"
        id="album"
        autocomplete="off"
      />
      <label class="btn btn-outline-primary" for="album">ALBUM</label>
</div>
    </div>

